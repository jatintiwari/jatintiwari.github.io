{
  "name": "@jatintiwari/portfolio",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv . --no-clear",
    "start-thoughts": "sirv thoughts --dev --single",
    "dev-thoughts": "NODE_ENV=thoughts rollup -c -w",
    "compile-home": "export templateName=index && export jsFileName=bundle && export cssFileName=bundle && node compile-handlebars",
    "watch-home": "yarn nodemon -w dist --exec \"yarn compile-home\""
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "handlebars": "^4.7.7",
    "mdsvex": "^0.10.6",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.16",
    "rollup-plugin-scss": "^3.0.0",
    "sirv-cli": "^2.0.0",
    "svelte-navigator": "^3.1.6",
    "svelte-preprocess": "^4.10.5"
  }
}
