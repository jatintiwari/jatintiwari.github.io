{
    "name": "@jatintiwari/portfolio",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "start": "sirv . --no-clear",
        "compile-home": "export templateName=index && export jsFileName=bundle && export cssFileName=bundle && node compile-handlebars",
        "watch-home": "yarn nodemon -w dist --exec \"yarn compile-home\"",
        "postinstall": "husky install"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^17.0.0",
        "@rollup/plugin-node-resolve": "^11.0.0",
        "rollup": "^2.3.4",
        "rollup-plugin-css-only": "^3.1.0",
        "rollup-plugin-livereload": "^2.0.0",
        "rollup-plugin-svelte": "^7.0.0",
        "rollup-plugin-terser": "^7.0.0",
        "svelte": "^3.49.0"
    },
    "dependencies": {
        "@jackfranklin/rollup-plugin-markdown": "^0.4.0",
        "handlebars": "^4.7.7",
        "husky": "^8.0.1",
        "mdsvex": "^0.10.6",
        "node-sass": "^7.0.1",
        "nodemon": "^2.0.16",
        "rollup-plugin-scss": "^3.0.0",
        "showdown": "^2.1.0",
        "sirv-cli": "^2.0.0",
        "svelte-navigator": "^3.1.6",
        "svelte-preprocess": "^4.10.5"
    }
}
